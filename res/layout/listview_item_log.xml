<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView 
        android:id="@+id/textview_id_log_listview_item_log_id"
        android:layout_width="50dp"
        android:layout_height="match_parent"
        android:text="@string/default_value_of_textview" />
    
    <TextView 
        android:id="@+id/textview_id_log_listview_item_log_datetime"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:text="@string/default_value_of_textview" />

    <TextView
        android:id="@+id/textview_id_log_listview_item_log_content"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/default_value_of_textview" />
    
</LinearLayout>
